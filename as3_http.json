 {
 	"class": "AS3",
 	"action": "deploy",
 	"persist": true,
 	"declaration": {
 		"class": "ADC",
 		"schemaVersion": "3.6.0",
 		"id": "testid",
 		"label": "HTTp Config",
 		"remark": "Simple HTTP Service with Round-Robin Load Balancing",
 		"VS_tenant": {
 			"class": "Tenant",
 			"A1": {
 				"class": "Application",
 				"template": "http",
 				"serviceMain": {
 					"class": "Service_HTTP",
 					"virtualAddresses": [
 						"10.0.1.10"
 					],
 					"pool": "web_pool",

 					"snat": {
 						"bigip": "/Common/sp1"
 					}
 				},
 				"web_pool": {
 					"class": "Pool",
 					"monitors": [
 						"http"
 					],
 					"members": [{
 						"servicePort": 80,
 						"serverAddresses": [
 							"192.0.1.10",
 							"192.0.1.11"
 						]
 					}]
 				}
 			}
 		}
 	}
 }
